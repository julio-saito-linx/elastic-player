<div class="row control-line control-buttons">
      <button class="btn btn-default btnPrev">
        <span class="glyphicon glyphicon-fast-backward"></span>
      </button>
      <button class="btn btn-default btnPlay">
        <span class="glyphicon glyphicon-play"></span>
      </button>
      <button class="btn btn-default btnNext">
        <span class="glyphicon glyphicon-fast-forward"></span>
      </button>
</div>
<div class="row control-line">
  <div class="col-lg-1 slider-label">Volume:</div>
  <div class="col-lg-11">
    <input type="range" id="volumeSlider"></input>
  </div>
</div>
<div class="row control-line">
  <div class="col-lg-1 slider-label">Time:</div>
  <div class="col-lg-11">
    <input type="range" id="timeSlider"></input>
  </div>
</div>

